    "axios": "0.19.2",
    "cheerio": "1.0.0-rc.3",
    "colors": "^1.4.0",
    "connect-timeout": "^1.9.0",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "graphql": "15.1.0",
