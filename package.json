{
	"name": "nepaltoday.api",
	"version": "1.0.0",
	"description": "A graphql based api endpoint for NepalToday mobile app",
	"main": "src/server.js",
	"scripts": {
		"start": "pm2-runtime start ecosystem.config.js",
		"format": "prettier --write \"src/**/*.js\"",
		"test": "jest --testMatch '**/?(*.)+(spec|test).[tj]s?(x)' --detectOpenHandles --forceExit",
		"test:coverage": "jest --coverage --testMatch '**/?(*.)+(spec|test).[tj]s?(x)'  --detectOpenHandles --forceExit",
		"source:insert": "node ./src/db-service/scripts/source-data-caller.js"
	},
	"dependencies": {
		"@bearer/node-agent": "2.0.7",
		"agenda": "3.0.0",
		"apollo-server-express": "2.11.0",
		"axios": "0.19.2",
		"cheerio": "1.0.0-rc.3",
		"colors": "^1.4.0",
		"connect-timeout": "^1.9.0",
		"dotenv": "8.2.0",
		"express": "^4.17.1",
		"graphql": "14.6.0",
		"graphql-tools": "4.0.7",
		"helmet": "^3.22.0",
		"html-to-text": "^5.1.1",
		"ip": "1.1.5",
		"moment": "2.24.0",
		"moment-timezone": "0.5.28",
		"mongodb": "3.5.5",
		"mongoose": "5.9.5",
		"morgan": "1.10.0",
		"newrelic": "6.5.0",
		"offline-geo-from-ip": "^1.0.1",
		"pm2": "4.2.3",
		"require-graphql-file": "1.0.1",
		"twitter": "1.7.1",
		"winston": "3.2.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/node": "^7.8.7",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.9.0",
		"babel-jest": "^25.1.0",
		"babel-loader": "^8.1.0",
		"casual": "^1.6.2",
		"eslint": "6.8.0",
		"eslint-config-standard": "14.1.1",
		"eslint-plugin-import": "2.20.1",
		"eslint-plugin-node": "11.0.0",
		"eslint-plugin-promise": "4.2.1",
		"eslint-plugin-standard": "4.0.1",
		"graphql-import": "^1.0.1",
		"jest": "^25.1.0",
		"mockingoose": "^2.13.2",
		"mongodb-memory-server": "6.4.1",
		"nock": "^12.0.3",
		"prettier": "1.19.1"
	},
	"engines": {
		"node": ">=v10.9.0",
		"npm": ">=6.2.0"
	},
	"keywords": [
		"NepalToday",
		"Nepal News",
		"Nepal Politician",
		"Nepal Celebrities"
	],
	"author": "Siris Technology",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/syuraj/nepaltoday-api/issues"
	}
